{
  "name": "deepdub-tts",
  "version": "0.1.0",
  "description": "Generate speech audio using Deepdub TTS API",
  "main": "SKILL.md",
  "keywords": ["tts", "deepdub", "voice", "speech", "audio"],
  "author": "Deepdub",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/deepdub/deepdub-tts-skill.git"
  },
  "homepage": "https://deepdub.ai",
  "openclaw": {
    "skill": true,
    "requirements": {
      "bins": ["python3"],
      "env": {
        "DEEPDUB_API_KEY": "required",
        "DEEPDUB_VOICE_PROMPT_ID": "required",
        "DEEPDUB_LOCALE": "optional",
        "DEEPDUB_MODEL": "optional",
        "OPENCLAW_MEDIA_DIR": "optional"
      }
    }
  }
}
