{
  "version": 1,
  "timezone": "Europe/Stockholm",

  "google": {
    "gcalcliPath": "gcalcli",
    "calendars": [
      { "label": "Primary", "name": "you@example.com" },
      { "label": "Family", "name": "Family" }
    ],
    "defaultWriteCalendarLabel": "Primary"
  },

  "caldav": {
    "enabled": false,
    "khalBin": "khal",
    "vdirsyncerSyncCommand": ["vdirsyncer", "sync", "icloud_calendar"],
    "khalCalendars": [
      { "label": "iCloud Family", "name": "CALENDAR_ID_FROM_KHAL_PRINTCALENDARS" }
    ]
  },

  "reminders": {
    "leadMinutes": 15,
    "ignoreBirthdays": true,
    "allDay": {
      "timeLocal": "09:00",
      "remindIfKeywords": [
        "flight",
        "appointment",
        "doctor",
        "dentist",
        "therapy",
        "school",
        "pickup",
        "dropoff",
        "deadline",
        "payment",
        "invoice",
        "rent",
        "car"
      ]
    }
  }
}
